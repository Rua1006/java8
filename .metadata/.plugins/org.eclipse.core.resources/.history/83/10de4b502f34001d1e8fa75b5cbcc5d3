package sec3;

public class Loop8 {

	public static void main(String[] args) {
		// 이중 for문을 활용하여 아래와 같은 형태의 구구단을 출력
/*
		2*1=2 3*1=3 ......... 9*1=9
		2 2
		............................
		2*9=18 ..........		
*/
	for(int i=1; i<=9; i++){
		System.out.println("구구단");
		for(int j=2; j<=9; j++){
        System.out.print(j+"x"+i+"="+(i*j)+"\t");
		System.out.print(j+"단"+"\t");
		}

	}
	}

}
