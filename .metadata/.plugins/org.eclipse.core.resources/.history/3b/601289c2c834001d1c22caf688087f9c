package sec4;

public class Ex6 {

	public static void main(String[] args) {
		for(int a=1;a<6;a++){
			for(int b=0; b<a; b++){
				System.out.print("*");
			}
				System.out.println();
		}
	System.out.println("\n");
		for(int a=5;a>0;a--){
			for(int b=1;b<a;b--){
			System.out.print("*");
		}
	}
	}
	}