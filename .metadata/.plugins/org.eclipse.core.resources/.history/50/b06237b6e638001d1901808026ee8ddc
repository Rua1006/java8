package sec2;

public class StudentEx {

	public static void main(String[] args) {
		Student studen1 = new Student();
		
		Student student1 = new Student("김기태",90,80,90);
		
		int a=student1.calcTot();
		System.out.println("점수합계 :"+a);
		
		int b=student1.calcAvg();
		System.out.println("점수평균 :"+b);
		
		String c=student1.calcHak();
		System.out.println("학점 :"+c);
		
		String d=student1.calcRes();
		System.out.println("판정 :"+d);
	}

}
