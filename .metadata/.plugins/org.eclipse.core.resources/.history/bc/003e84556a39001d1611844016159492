package sec2;

public class AccountEX {

	public static void main(String[] args) {
		Account account = new Account();
		
		account.runDeposit();
		System.out.println("입금 : "+account.runDeposit());
		
		account.runWithdraw();
		System.out.println("출금: "+account.runWithdraw());

		account.runBalance();
		System.out.println("잔액조회"+account.runBalance());
	}

}
