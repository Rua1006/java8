package sec3;

public class test {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
    	System.out.println("구구단");
		for(int e=1; e<=9; e++){
			for(int f=2; f<=9; f++){
				if(e==1){
					System.out.println(f+"단"+"\t\t");
				}
				else{
					System.out.println(f+"x"+e+"="+(e*f)+"\t");
	}

			}
		System.out.println();	
		}
}
}