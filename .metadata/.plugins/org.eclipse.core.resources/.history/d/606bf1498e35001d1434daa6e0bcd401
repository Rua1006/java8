package sec1;

public class Type3 {

	public static void main(String[] args) {
		String name = "kang"; //참조형이지만 완벽한 참조형이 아님
		char[] irum = {'k','a','n','g'};
		String nick = "kang";
		System.out.println(name);
		System.out.println(irum);
		//System.out.println(name==irum); - 서로 비교대상이 될 수 없음
		System.out.println(name==nick);
		System.out.println(name!=nick);
	}

}
