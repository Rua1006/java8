package sec2;

public class Product {
	String pid;
	String pname;
	int amount;
	int price;
	String img;
	
	public Product(){}
	public Product(String pid, String pname) {
		this.pid = pid;
		this.pname = pname;
	}
	public Product(String pid, String pname, int amount, int price) {
		super();
		this.pid = pid;
		this.pname = pname;
		this.amount = amount;
		this.price = price;
	}
	
}
